subset(select = -c(1:4, 7, 9:12, 14:26))
healthy_life_m_tidy <- healthy_life_expec_m %>%
subset(select = -c(1:4, 7, 9:12, 14:26))
maternity_care_tidy <- maternity_care %>%
subset(select = -c(1:4, 7:12, 14:26))
